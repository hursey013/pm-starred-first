{
  "manifest_version": 2,
  "name": "Starred First for ProtonMail",
  "description": "Moved starred ProtonMail messages to the top of the inbox",
  "version": "0.0.3",
  "author": "Brian Hurst",
  "icons": {
    "48": "icon48.png"
  },
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://mail.protonmail.com/inbox*"],
    "run_at": "document_end"
  }],
  "permissions": ["tabs"]
}
